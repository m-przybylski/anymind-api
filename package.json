{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@anymind-ng/api",
  "version": "1.0.0",
  "description": "swagger client for anymind",
  "author": "AnyMind",
  "keywords": [
    "swagger-client"
  ],
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "license": "Unlicense",
  "typings": "dist/index.d.ts",
  "repository": "git@git.contactis.pl:itelo/anymind-ng-api.git",
  "scripts": {
    "version": "npm run build && git add . && ./commit-msg.sh",
    "build": "npm run gen && ng build",
    "publish": "npm run build && npm publish ./dist/api",
    "gen": "npm run clean && node projects/api/generator/index.js --url 'https://stage.anymind.com/api/docs/swagger/swagger.json'",
    "ci-test": "npm run gen && ng build",
    "clean": "rimraf projects/api/src/api projects/api/src/model"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/core": "^7.2.1",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.0",
    "mustache": "^3.0.1",
    "ng-packagr": "^4.2.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.1",
    "protractor": "~5.4.0",
    "rxjs": "^6.3.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "registry": "https://nexus.anymind.com/repository/npm-private"
  }
}
