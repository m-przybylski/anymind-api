{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@anymind-ng/api",
  "version": "0.0.54",
  "description": "swagger client for anymind",
  "author": "AnyMind",
  "keywords": [
    "swagger-client"
  ],
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "license": "Unlicense",
  "typings": "dist/index.d.ts",
  "repository": "git@git.contactis.pl:itelo/anymind-ng-api.git",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish:lib": "npm publish ./dist",
    "publish:lib:next": "npm publish --tag next ./dist",
    "gen": "npm run clean && swagger-codegen generate -i https://stage.anymind.com/api/docs/swagger/swagger.json -l typescript-angular -t typescript-angular",
    "ci-test": "npm run build",
    "clean": "rimraf api model"
  },
  "peerDependencies": {
    "@angular/common": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/http": "^5.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/common": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/http": "^5.2.0",
    "zone.js": "^0.8.14",
    "ng-packagr": "2.0.0-rc.11",
    "typescript": "~2.4.2",
    "rimraf": "^2.6.2"
  },
  "publishConfig": {
    "registry": "https://nexus.contactis.pl/repository/npm-internal"
  }
}
